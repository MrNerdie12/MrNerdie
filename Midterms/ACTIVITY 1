public class Diamond {

  public static void main (String [] args) {

    System.out.print("\t  *  \n\t * * \n\t* * *\n\t * * \n\t  *  ");

  }
}
public class Initial {

  public static void main (String [] args) {

    System.out.println("IIIII\tM   M\t QQQQ");
    System.out.println("  I  \tMM MM\tQ    Q");
    System.out.println("  I  \tM M M\tQ    Q");
    System.out.println("  I  \tM   M\tQ  Q Q");
    System.out.println("IIIII\tM   M\t QQQQ Q");

  }
}
public class MyInfo {

  public static void main (String [] args) {

    System.out.println("Name:\t\tIan Angelo M. Quitaleg");
    System.out.println("Course:\t\tBS IT");
    System.out.println("Year:\t\t1");
    System.out.println("Address:\tMarcos Highway");
    System.out.println("Adjective:\tCute");

  }
}
public class Trees {

   public static void main (String [] args) {

    System.out.println("   X");
    System.out.println("  XXX");
    System.out.println(" XXXXX");
    System.out.println("XXXXXXX");
    System.out.println("   X");

  }
}
